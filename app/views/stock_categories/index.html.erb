<h1>Listing stock_categories</h1>
<%= StockDayHisData.count %>
<table>
  <tr>
    <th>ID</th>
    <th>Name</th>
    <th></th>
    <th></th>
    <th></th>
  </tr>

<% @stock_categories.each do |stock_category| %>
  <tr>
    <td><%= stock_category.id %></td>
    <td><%= stock_category.name %></td>
    <td><%= link_to 'Show', stock_category %></td>
    <td><%= link_to 'Edit', edit_stock_category_path(stock_category) %></td>
    <td><%= link_to 'Destroy', stock_category, :confirm => 'Are you sure?', :method => :delete %></td>
  </tr>
<% end %>

</table>

